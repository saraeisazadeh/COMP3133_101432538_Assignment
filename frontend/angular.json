        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "101432538_comp3133_assignment2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "101432538_comp3133_assignment2:build:production"
            },
            "development": {
              "browserTarget": "101432538_comp3133_assignment2:build:development"
            }
          },
          "defaultConfiguration": "development"
